  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Cursor</Title>
      <Shortcut></Shortcut>
      <Description>Sample cursor script.</Description>
      <Author>Jan Timr, Roman Å tefko</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>Variable</ID>
          <ToolTip>Variable fetched from the cursor</ToolTip>
          <Default>Variable</Default>
        </Literal>
        <Literal>
          <ID>VariableType</ID>
          <ToolTip>Data type of the variable</ToolTip>
          <Default>UNIQUEIDENTIFIER</Default>
        </Literal>
        <Literal>
          <ID>CursorName</ID>
          <ToolTip>Name of the cursor</ToolTip>
          <Default>CursorName</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL"><![CDATA[DECLARE @$Variable$ AS $VariableType$
DECLARE $CursorName$ CURSOR LOCAL FORWARD_ONLY FAST_FORWARD READ_ONLY FOR
	-- TODO: Add SELECT statement
	OPEN $CursorName$

	FETCH NEXT FROM $CursorName$ INTO @$Variable$
	WHILE @@FETCH_STATUS = 0
	BEGIN
		-- TODO: Add cursor logic

		FETCH NEXT FROM $CursorName$ INTO @$Variable$
	END
CLOSE $CursorName$
DEALLOCATE $CursorName$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
